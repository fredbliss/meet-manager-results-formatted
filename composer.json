{
    "name": "intelligentspark/meet-manager-results-formatted",
    "description": "A tool for formatting meet manager results from a generic Access database export for The Greenfield Recorder",
    "keywords": ["meet manager","export","reporting","project"],
    "homepage": "http://github.com/intelligentspark/meet-manager-results-formatted",
    "license": "UNLICENSED",
    "authors": [
        {
            "name": "Fred Bliss",
            "email": "fredrbliss@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "josegonzalez/dotenv": "dev-master",
        "aura/sql": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public public/index.php",
        "test": "phpunit"
    }

}
